<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Tabbed" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#FA0601</item>
        <item name="colorPrimaryVariant">#FF005D</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/teal_200</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">#F8C836</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">#3C9CF1</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="Theme.Tabbed.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.Tabbed.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.Tabbed.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
</resources>